END SUB


SUB MRAK (X, Y)

    LINE (0 + X, 0 + Y)-(10 + X, 10 + Y), 30, BF
    LINE (0 + X, 10 + Y)-(30 + X, 20 + Y), 30, BF
    LINE (10 + X, 20 + Y)-(20 + X, 30 + Y), 30, BF


END SUB




